<h1>Listing users</h1>

<table>
  <tr>
    <th>Orgid</th>
    <th>Userid</th>
    <th>Name</th>
    <th>Email</th>
    <th>Admin</th>
    <th>Judge</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.orgid %></td>
    <td><%= user.userid %></td>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.admin %></td>
    <td><%= user.judge %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
